<%- include ('../partials/header') %>
    <%- include ('../partials/navbar') %>
        <main>
            <div class="container">
                <!-- THE REST OF YOUR HTML CODE GOES HERE -->
				<h1>Welcome to the Homepage</h1>
				<table>
					<thead>
						<tr>
							<th>User ID</th>
							<th>Username</th>
							<th>Other Info</th>
						</tr>
					</thead>
					<tbody>
						<% users.forEach(user => { %>
							<tr>
								<td><%= user.user_id %></td>
								<td><%= user.username %></td>
								<td><%= user.other_info %></td>
							</tr>
						<% }); %>
					</tbody>
				</table>
            </div>
        </main>


        <%- include ('../partials/footer') %>